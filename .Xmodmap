!GENERATE DEFAULT XMODMAP FIRST USING
!$ xmodmap -pke > ~/.Xmodmap


! CapsLock -> Mode_switch
keycode 66 = Mode_switch

keycode 44 = j J Down
keycode 45 = k K Up
keycode 43 = h H Left
keycode 46 = l L Right
keycode 47 = semicolon colon odiaeresis Odiaeresis
keycode 48 = apostrophe quotedbl adiaeresis Adiaeresis
